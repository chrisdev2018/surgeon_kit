<odoo>
    <data noupdate="1">

        <record id="surgery_entry_sequence" model="ir.sequence">
           <field name="name">Surgery entry code</field>
           <field name="code">surgery_entry.code</field>
           <field name="prefix">SEC%(y)s%(month)s%(day)s</field>
           <field name="active">TRUE</field>
           <field name="padding">3</field>
           <field name="number_next">1</field>
           <field name="number_increment">1</field>
        </record>

    </data>
</odoo>